<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.itis.models.Actor" catalog="public" table="actors">
        <id name="id" column="id" type="java.lang.Integer">
            <generator class="identity"/>
        </id>
        <property name="idFilm" column="id_film"/>
        <property name="actorName" column="actor_name"/>
    </class>
</hibernate-mapping>